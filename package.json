{
  "name": "kompo-ui",
  "version": "0.0.1",
  "description": "A collection of interface components (such as filterlists, datepickers or toolbars) created with the Kompo library",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "npm run build:cjs && npm run build:umd && npm run build:min",
    "build:cjs": "./node_modules/.bin/babel ./src -d lib",
    "build:umd": "./node_modules/.bin/webpack --entry ./components/index.js --output-filename ./umd/kompo.js",
    "build:min": "./node_modules/.bin/webpack -p --entry ./components/index.js --output-filename ./umd/kompo.min.js",
    "components:build": "./node_modules/.bin/webpack --config components.webpack.js --optimize-minimize",
    "components:watch": "./node_modules/.bin/webpack --config components.webpack.js -w"
  },
  "keywords": [
    "Kompo",
    "interface",
    "components",
    "ui",
    "user",
    "interface"
  ],
  "author": "Ruben Hazelaar <ruben.hazelaar@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.16.0",
    "babel-loader": "^6.2.5",
    "babel-plugin-syntax-flow": "^6.13.0",
    "babel-plugin-transform-flow-strip-types": "^6.14.0",
    "babel-plugin-transform-object-assign": "^6.8.0",
    "babel-preset-es2015": "^6.16.0",
    "webpack": "^2.1.0-beta.25"
  },
  "dependencies": {
    "kompo": "^1.0.3",
    "kompo-util": "^1.1.0"
  }
}
